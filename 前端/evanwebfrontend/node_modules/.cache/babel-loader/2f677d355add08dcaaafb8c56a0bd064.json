AAA,SAAA,CAAA,SAFA,+BAEA,KAAA,SAAA,CAAA,gBAFA,GAGA;AACA,QAAA,OAAA,EAAA;AACA,UAAA,aAAA,EAAA,cAAA,CAAA,OAAA,CAAA,eAAA;AADA;AADA,OAHA,EASA,IATA,CASA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,MAAA,IAAA,GAAA,EAAA;AACA,iBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CAAA;AACA;;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,MAAA,EAAA,GAAA;AACA,QAAA,KAAA,CAAA,iBAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,QAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,KAAA;AACA,OAhBA,EAiBA,KAjBA,CAiBA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,OAAA,CAAA,OAAA,CAAA,KAAA,MAAA,CAAA,CAAA,EAAA;AACA,iBAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,0BAAA,CAAA;AACA;;AACA,eAAA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,qBAAA,CAAA;AACA,OAtBA;AAuBA,KAzBA;AA0BA;AACA,IAAA,gBA3BA,4BA2BA,OA3BA,EA2BA;AACA,WAAA,SAAA,CAAA,QAAA,GAAA,OAAA;AACA,WAAA,gBAAA;AACA,KA9BA;AA+BA;AACA,IAAA,mBAhCA,+BAgCA,OAhCA,EAgCA;AACA,WAAA,SAAA,CAAA,SAAA,GAAA,OAAA;AACA,WAAA,gBAAA;AACA;AAnCA,GAZA;AAiDA,EAAA,OAjDA,qBAiDA;AACA,SAAA,gBAAA;AACA;AAnDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/Home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>公司管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>公司信息</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n\r\n    <!-- 卡片视图 -->\r\n    <el-card class=\"box-card\">\r\n      <el-row :gutter=\"10\">\r\n        <el-col :span=\"7\">\r\n          <el-input placeholder=\"请输入公司名称\" clearable v-model=\"queryInfo.querycompanyName\">\r\n            <el-button\r\n              slot=\"append\"\r\n              icon=\"el-icon-search\"\r\n              @click=\"GetCompaniesList\"\r\n            ></el-button> </el-input\r\n        ></el-col>\r\n        <el-col :span=\"4\">\r\n          <el-button type=\"primary\">添加公司</el-button>\r\n        </el-col>\r\n      </el-row>\r\n\r\n      <!-- 表格区域 -->\r\n      <div class=\"Data\">\r\n        <el-table :data=\"ListCompaniesInfo\" border>\r\n          <el-table-column type=\"index\" label=\"#\"> </el-table-column>\r\n          <el-table-column prop=\"id\" label=\"GUID\" width=\"300\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"name\" label=\"公司名\" width=\"180\">\r\n          </el-table-column>\r\n          <el-table-column prop=\"introduction\" label=\"公司描述\">\r\n          </el-table-column>\r\n\r\n          <el-table-column label=\"操作\">\r\n            <!-- 作用与插槽 -->\r\n            <template v-solt=\"scoped\">\r\n              <el-tooltip effect=\"dark\" content=\"编辑\" placement=\"top\" :hide-after=\"0\" > \r\n                <el-button type=\"warning \" icon=\"el-icon-edit\"></el-button>\r\n              </el-tooltip>\r\n\r\n              <el-tooltip effect=\"dark\" content=\"删除\" placement=\"top\" :hide-after=\"0\" >\r\n                <el-button type=\"danger\" icon=\"el-icon-delete\"></el-button>\r\n              </el-tooltip>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n\r\n      <!-- 分页区域 -->\r\n      <el-pagination\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :current-page=\"queryInfo.pageindex\"\r\n        :page-sizes=\"[1, 2, 5, 10]\"\r\n        :page-size=\"queryInfo.pageSize\"\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :total=\"total\"\r\n      >\r\n      </el-pagination>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      queryInfo: {\r\n        querycompanyName:'',  \r\n        pageSize: 2,\r\n        pageindex: 1,\r\n      },\r\n      ListCompaniesInfo: [],\r\n      total: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    GetCompaniesList() {\r\n      this.$http\r\n        .get(\r\n          `/api/CEGC/Companies/GetCompanies?pageSize=${this.queryInfo.pageSize}&pageindex=${this.queryInfo.pageindex}&querycompanyName=${this.queryInfo.querycompanyName}`,\r\n          {\r\n            headers: {\r\n              Authorization: sessionStorage.getItem(\"Authorization\"),\r\n            },\r\n          }\r\n        )\r\n        .then((res) => {\r\n          if (res.status != 200) {\r\n            return this.$message.error(\"查询公司信息失败\");\r\n          }\r\n          console.log(\"公司数据\", res);\r\n          this.ListCompaniesInfo = res.data.data;\r\n          this.total = res.data.total;\r\n        })\r\n        .catch((err) => {\r\n          if (err.message.indexOf(\"401\") !== -1) {\r\n            return this.$message.error(\"accessToken不正确或已过期,请重新登陆\");\r\n          }\r\n          return this.$message.error(\"查询公司信息失败,请查看服务器是否开启\");\r\n        });\r\n    },\r\n    // 监听pageSize的改变事件\r\n    handleSizeChange(newSize) {\r\n      this.queryInfo.pageSize = newSize;\r\n      this.GetCompaniesList();\r\n    },\r\n    // 监听pageindex的改变事件\r\n    handleCurrentChange(newpage) {\r\n      this.queryInfo.pageindex = newpage;\r\n      this.GetCompaniesList();\r\n    },\r\n  },\r\n  created() {\r\n    this.GetCompaniesList();\r\n  },\r\n};\r\n</script>\r\n\r\n<style style=\"less\" scoped>\r\n.Data {\r\n  margin-top: 30px;\r\n}\r\n</style>"],"sourceRoot":"src/components/Companies"}]}